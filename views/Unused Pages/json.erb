 	<script type="text/javascript">

 		function upperCase(x)
		{
			alert("123")
			var y=document.getElementById(x).value
			document.getElementById(x).value=y.toUpperCase()
		}

		function loadInfo() {
			alert('come on');
			
 		$.getJSON("/submitjson",function(data){
 			$("#info").html("");
 			$.each(data,function(i, item){
 				$("#info").append('<a href="/searchflow/'+item.app_name+
 					'" class="list-group-item" name="appname">'+item.app_name+'</a>'
 					);
 			})
 		})
		}
	</script>

